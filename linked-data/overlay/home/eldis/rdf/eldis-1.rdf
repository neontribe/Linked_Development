  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64069/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64069/full/mass-media-exposure-and-its-impact-on-fertility-current-scenario-of-bangladesh/"/>
