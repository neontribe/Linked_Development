    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64853/full/globalization-a-way-of-prospects-and-threats-for-the-economy-of-bangladesh/"/>
    <dcterms:date>2010-01-01T00:00:00</dcterms:date>
