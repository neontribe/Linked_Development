  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64640/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64640/full/e-government-websites-in-bangladesh-a-study-on-citizens-benefits/"/>
