  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64081/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64081/full/impact-of-ict-on-gender-gap-in-egypt/"/>
