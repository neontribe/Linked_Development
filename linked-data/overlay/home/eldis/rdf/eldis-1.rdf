    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A63998/full/etiology-and-risk-factors-of-febrile-seizure-an-update/"/>
    <dcterms:date>2010-01-01T00:00:00</dcterms:date>
    <rdf:type rdf:resource="http://purl.org/ontology/bibo/Article"/>
    <dcterms:title>Etiology and Risk Factors of Febrile Seizure â€“ An Update</dcterms:title>
