    <dcterms:language>English</dcterms:language>
  </rdf:Description>
  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A63571/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A63571/full/india-africa-economic-partnership-trends-and-prospects-research-and-information-system-for-developing-countries/"/>
