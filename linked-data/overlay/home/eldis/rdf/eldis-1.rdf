    <dcterms:title>Prevention and Treatment of HIV and Other Sexually Transmitted Infections for Sex Workers in Low- and Middle- Income Countries</dcterms:title>
    <dcterms:type rdf:resource="http://purl.org/dc/terms/Text"/>
