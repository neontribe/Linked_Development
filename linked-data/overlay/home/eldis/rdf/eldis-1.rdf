    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A63861/full/application-of-electronic-banking-in-bangladesh-an-overview/"/>
    <dcterms:date>2010-01-01T00:00:00</dcterms:date>
