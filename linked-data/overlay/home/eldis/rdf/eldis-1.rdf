  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64343/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64343/full/information-and-communication-technologies/"/>
    <dcterms:date>2012-01-01T00:00:00</dcterms:date>
