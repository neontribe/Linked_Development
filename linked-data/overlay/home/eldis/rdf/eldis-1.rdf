    <dcterms:publisher>Overseas Development Institute</dcterms:publisher>
    <dcterms:creator>N. Bird</dcterms:creator>
    <dcterms:language>English</dcterms:language>
  </rdf:Description>
  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64286/">
