  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A63752/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A63752/full/nipah-virus-endemic-in-bangladesh/"/>
    <dcterms:date>2011-01-01T00:00:00</dcterms:date>
