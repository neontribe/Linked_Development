    <dcterms:creator>M. Hunter</dcterms:creator>
    <dcterms:language>English</dcterms:language>
  </rdf:Description>
  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64071/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64071/full/patriarchy-and-women-s-subordination-a-theoretical-analysis/"/>
