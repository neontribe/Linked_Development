  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A63762/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A63762/full/msm-country-snapshot-series-viet-nam/"/>
    <dcterms:date>2012-01-01T00:00:00</dcterms:date>
