    <dcterms:date>2011-01-01T00:00:00</dcterms:date>
    <rdf:type rdf:resource="http://purl.org/ontology/bibo/Article"/>
    <dcterms:title>A Stakeholder Map forClimate Change Adaptation in Bangladesh’s Agricultural Sector</dcterms:title>
