    <dcterms:publisher>Canadian Center of Science and Education</dcterms:publisher>
    <dcterms:creator>H. E. Batta</dcterms:creator>
    <dcterms:coverage rdf:resource="http://linked-development.org/eldis/countries/A1159/"/>
    <dcterms:language>English</dcterms:language>
  </rdf:Description>
  <rdf:Description rdf:about="http://linked-development.org/eldis/output/A64606/">
    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A64606/full/strategy-framework-for-promoting-ict-literacy-in-the-asia-pacific-region/"/>
