    <dcterms:identifier rdf:resource="http://api.ids.ac.uk/openapi/eldis/get/documents/A63603/full/smart-nutrition-survey-in-northern-nigeria/"/>
    <dcterms:date>2010-01-01T00:00:00</dcterms:date>
